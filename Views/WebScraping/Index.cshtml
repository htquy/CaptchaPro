@model Captcha.Models.TTModel

<!-- Định nghĩa form với method POST -->
<form asp-action="Submit" method="post">
    <!-- Tạo các ô input dựa trên thuộc tính của model TTModel -->
    <!-- Ví dụ: Nếu TTModel có thuộc tính 'Username' và 'Password' -->
    <div>
        <label for="Fullname">Username:</label>
        <input type="text" id="Fullname" name="Fullname" value="@Model?.Fullname" />
    </div>
    <div>
        <label for="Age">Age:</label>
        <input type="number" id="Age" name="Age" value="@Model?.Age" />
    </div>
    <div>
        <label for="Sexual">Sexual:</label>
        <input type="text" id="Sexual" name="Sexual" value="@Model?.Sexual" />
    </div>
    <div>
        <label for="Country">Country:</label>
        <input type="text" id="Country" name="Country" value="@Model?.Country" />
    </div>
    <div>
        <label for="Password">Password:</label>
        <input type="password" id="Password" name="Password" value="@Model?.Password" />
    </div>

    <!-- Các ô input khác dựa trên thuộc tính của TTModel -->
    <!-- ... -->
    <!-- Nút submit form -->
    <div>
        <button type="submit" name="submit">Submit</button>
    </div>
</form>
