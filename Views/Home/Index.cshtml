
@{
    ViewData["Title"] = "Index";
    var dstt = ViewBag.tt as IEnumerable<Captcha.Datas.TT>;
}

<h1>Index</h1>

<div>
    <h4>TT</h4>
    <hr />
    @foreach(var item in dstt){
    <dl class="row">
        <dt class = "col-sm-2">
            Id
        </dt>
        <dd class = "col-sm-10">
            @item.Id
        </dd>
        <dt class = "col-sm-2">
            Họ tên
        </dt>
        <dd class = "col-sm-10">
            @item.Fullname
        </dd>
        <dt class = "col-sm-2">
            Tuổi
        </dt>
        <dd class = "col-sm-10">
            @item.Age
        </dd>
        <dt class = "col-sm-2">
            Giới tính
        </dt>
        <dd class = "col-sm-10">
            @item.Sexual
        </dd>
        <dt class = "col-sm-2">
            Quốc gia
        </dt>
        <dd class = "col-sm-10">
            @item.Country
        </dd>
        <dt class = "col-sm-2">
            Mật khẩu
        </dt>
        <dd class = "col-sm-10">
            @item.Password
        </dd>
    </dl>}
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
